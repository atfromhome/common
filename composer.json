{
    "name": "fromhome/common",
    "description": "Common component at fromhome.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nuradiyana",
            "email": "me@nooradiana.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-pdo": "*",
        "laravel/framework": "^8.0",
        "ramsey/uuid": "^4.1",
        "webmozart/assert": "^1.9"
    },
    "require-dev": {
        "laravel/legacy-factories": "^v1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "FromHome\\Common\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FromHome\\Common\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
